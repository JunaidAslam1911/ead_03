@model IEnumerable<Prayer_Timings_Schedule_For_Masjid.Models.Masjid>

@{ViewBag.Title = "ShowData";}
    <table width="100%" border="1">
        <tr>
            <td>Address</td>
            <td>Name</td>
            <td>Area</td>
            <td>Fajar</td>
            <td>Zohar</td>
            <td>Asar</td>
            <td>Magrib</td>
            <td>Esha</td>
            <td>Edit</td>
        </tr>
        @{ 
            foreach (var s in Model)
            {
               
                <tr>
                    <td>@s.Address</td>
                    <td>@s.Name</td>
                    <td>@s.Area</td>
                    <td>@s.Fajar</td>
                    <td>@s.Zohar</td>
                    <td>@s.Asar</td>
                    <td>@s.Magrib</td>
                    <td>@s.Esha</td>
                    <td>
                        <a href="/Home/Edit/@s.Id">Edit</a>
                    </td>
                    
                </tr>
                

                
                
            }
        }

    </table>
